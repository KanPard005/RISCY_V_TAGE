#include <ooo_cpu.h>
#include "tage.h"

Tage tage_predictor;

void O3_CPU::initialize_branch_predictor()
{
    tage_predictor.init(cpu);
}

uint8_t O3_CPU::predict_branch(uint64_t ip)
{
    return tage_predictor.predict(cpu,ip);
}

void O3_CPU::last_branch_result(uint64_t ip, uint8_t taken)
{
    tage_predictor.update(cpu,ip,taken);
}
